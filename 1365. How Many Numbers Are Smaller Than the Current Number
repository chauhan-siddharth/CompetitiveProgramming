class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int[] output;
        int n=nums.length;
        output = new int[n];
       for(int i=0;i<n;i++){
           
           int min = nums[i];
           int count=0;
           for(int j=0;j<n;j++){              
               if(nums[i]>nums[j])
                   output[i]++;
           }
          // output[i]=count;
       }
    return output;
       }
}
        
    
